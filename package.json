{
  "name": "skyux-components",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "build": "nx run-many --target=build --all",
    "postbuild": "nx run-many --target=postbuild --all",
    "test": "nx run-many --target=test --all --parallel --maxParallel=4",
    "test:all": "nx run all:test --skip-nx-cache",
    "build:affected": "nx affected --target=build",
    "postbuild:affected": "nx affected --target=postbuild",
    "test:affected": "nx affected --target=test --parallel --maxParallel=4",
    "watch": "nx run-many --target=test --all --parallel --maxParallel=4 --watch",
    "watch:affected": "nx affected --target=test --parallel --maxParallel=4 --watch",
    "skyux:format": "npx prettier --write ."
  },
  "private": true,
